// Hello

#hi
	-> "Ну здорово" -> %greeted
	-> "Здорово" -> %greeted
	-> "Привет" -> %greeted
	-> "Хай" -> %greeted
	-> %greeted
	%greeted
		-> "Да привет, привет" -> %greeted2
		-> "Ну привет еще раз" -> %greeted2
		-> "Здорово еще раз" -> %greeted2
		-> "Я уже это слышал" -> %greeted2
		%greeted2
			-> "Здоровались уже" -> %greeted3
			-> "Здоровались уже, забыл@?" -> %greeted3
			%greeted3
				-> ""

// Undefined words

UD: ?
	-> "???" -> [UD $undef]
	-> "Какая-то чушь" -> [UD $undef]
	-> "Ты хоть сам@ понял@, что написал@?" -> [UD $undef] -> $are_you_ok
	-> "Просто набор букв" -> [UD $undef]
	-> "Ты бредишь" -> [UD $undef]
	-> "И что это должно значить?" -> [UD $undef]
	-> "Ты вообще о чем?" -> [UD $undef]
	-> [UD $undef]
	UD_LAST: $undef
		-> "Все еще бред" -> [UD $undef]
		-> "Ты умеешь пользоваться клавиатурой?" -> [UD $undef] -> $are_you_ok
		-> "Так ты не умеешь писать" -> [UD $undef]
		-> "Скажи сначала, что значит " -> UD_LAST $undef
		-> "Исправься" -> [UD $undef]
		-> "Очень осмысленно" -> [UD $undef]
		-> "Что ты хочешь этим сказать?" -> [UD $undef] -> $rhetorical
		$undef/1/2
			-> "Хватит спамить" -> [UD $undef]
			-> "Пиши нормально" -> [UD $undef]
			-> "Ты там в норме?" -> [UD $undef] -> $are_you_ok
			-> "С тобой все в порядке?" -> [UD $undef] -> $are_you_ok
			-> "Убери кота с клавиатуры" -> [UD $undef]
			-> "Да может перестанешь уже?" -> [UD $undef]
			$undef/2/3
				-> "..." -> $grudge
				-> "Это нелепо" -> $grudge
				-> "Просто абсурд" -> $grudge
				-> "Мне надоело это слушать" -> $grudge
				-> "Ты явно не дружишь с головой" -> $grudge
				-> "Это зашло слишком далеко" -> $grudge
	+$grudge/5, '^ UD $'
		-> $grudge

// Replies on answers

+$are_you_ok
	REPLY:
	#yes
		-> "Что " -> REPLY -> "?"
		-> "А мне кажется, что нет"
		-> "Так не выглядит"
		-> "Не верю"
		-> "Докажи"
	#no
		-> "Так и знал"
		-> "Оно и понятно"
		-> "Кто бы сомневался"
		-> "Мне стоило догадаться"
		-> "Ну да, как я мог забыть"

+$am_i_a_joke
	#yes; *laugh
		-> "Да " -> .пошел-verb-sl-3rd-pf-pg $insult -> " ты"
		-> "Соси " -> ~жопу-noun-acc $insult
		-> "А не пойти ли тебе" -> $insult
		-> "Ты стал много себе позволять" -> $insult
		-> "Рот ставишь?" -> $insult -> $are_you_a_joke
		-> "Мне не нужно твое ютубовское мнение" -> $insult
		-> "Засунь свое мнение себе в " -> ~жопу-noun-acc $insult
	#no
		-> "Именно"
		-> "Правильно"
		-> "Соображаешь"
		-> "Правильный ответ"
		-> "Ну вот и правильно"
		-> "Не забывай об этом"
		-> "Учитывай на будущее"
		-> "Всегда помни об этом"
		-> "Ну хоть что-то понимаешь"

+$are_you_a_joke
	#yes
		-> "Я был о тебе лучшего мнения"
		-> "Ну это просто позор"
		-> "Я разочарован"
		-> "Фу таким быть"
		-> "Ну логично"
		-> "Стыдоба"
		-> "Пффф"
		-> "Уууу"
	#no
		-> "Может тебе медаль за это дать?" -> $are_you_a_joke
		-> "Гордишься собой?" -> $are_you_a_joke
		-> "Хочешь поговорить об этом?" -> $would_you_like_to
